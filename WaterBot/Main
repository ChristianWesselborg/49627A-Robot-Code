#pragma config(Sensor, dgtl1,  Elbow,          sensorTouch)
#pragma config(Sensor, dgtl2,  Shol,           sensorTouch)
#pragma config(Sensor, dgtl3,  Button,             sensorTouch)
#pragma config(Motor,  port1,           rightMotor,    tmotorServoContinuousRotation, openLoop, reversed)
#pragma config(Motor,  port2,           SMotor,        tmotorServoContinuousRotation, openLoop, reversed)
#pragma config(Motor,  port3,           EMotor,        tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port4,           CMotor,        tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port5,           WMotor,        tmotorServoContinuousRotation, openLoop)
#pragma config(Motor,  port10,          leftMotor,     tmotorServoContinuousRotation, openLoop)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

/*
#pragma config(Motor,  port1,           rightMotor,    tmotorNormal, openLoop, reversed)
#pragma config(Motor,  port2,           SMotor,        tmotorNormal, openLoop, reversed)
#pragma config(Motor,  port3,           EMotor,        tmotorNormal, openLoop)
#pragma config(Motor,  port4,           CMotor,        tmotorNormal, openLoop)
#pragma config(Motor,  port5,           WMotor,        tmotorNormal, openLoop)
#pragma config(Motor,  port10,          leftMotor,     tmotorNormal, openLoop)
*/
task main()
{
	wait1Msec(2000);				// Robot waits for 2000 milliseconds before executing program
	
	int init = 0;
	
	while(init == 0)
	{
		init = 1;
	while(SensorValue(Button) == 0)
	{
	}
	
	wait1Msec(500);
	
	motor[leftMotor] = 127;
	motor[rightMotor] = 127;
	wait1Msec(7500);
	
	motor[leftMotor] = 0;
	motor[rightMotor] = 0;
	
	motor[SMotor] = -50;
	motor[EMotor] = -37;
	wait1Msec(9500);
	
	motor[SMotor] = 0;
	motor[EMotor] = 0;
	wait1Msec(1500);
	
	motor[CMotor] = 50;
	wait1Msec(1000);
	
	motor[CMotor] = 0;
	wait1Msec(500);
	
	motor[CMotor] = -30;
	wait1Msec(1000);
	
	motor[CMotor] = 0;
	
	
	while((SensorValue(Elbow) == 0) || (SensorValue(Shol) == 0))
	{
		if(SensorValue(Elbow) == 0)
			motor[EMotor] = 58;
		else
			motor[EMotor] = 0;
	
		if(SensorValue(Shol) == 0)
			motor[SMotor] = 43;
		else
			motor[SMotor] = 0;
	}
	motor[SMotor] = 0;
	motor[EMotor] = 0;
	wait1Msec(500);
	
	motor[leftMotor] = 40;
	motor[rightMotor] = -40;
	wait1Msec(2500);
	
	motor[leftMotor] = 0;
	motor[rightMotor] = 0;
	wait1Msec(500);
	
	motor[leftMotor] = 127;
	motor[rightMotor] = 127;
	wait1Msec(7500);
	
	motor[leftMotor] = 0;
	motor[rightMotor] = 0;
	
	motor[leftMotor] = 40;
	motor[rightMotor] = -40;
	wait1Msec(2950);
	
	motor[leftMotor] = 0;
	motor[rightMotor] = 0;
	wait1Msec(500);

	while(SensorValue(Button) == 0)
	{
	}
	init = 0;
}
}





